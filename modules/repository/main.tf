
module "repository" {
  source  = "mineiros-io/repository/github"
  version = "0.18.0"

  name                                  = var.name
  description                           = var.description
  homepage_url                          = var.homepage_url
  default_branch                        = var.default_branch
  gitignore_template                    = var.gitignore_template
  license_template                      = var.license_template
  defaults                              = var.defaults
  has_projects                          = var.has_projects
  vulnerability_alerts                  = var.vulnerability_alerts
  topics                                = var.topics
  pages                                 = var.pages
  is_template                           = var.is_template
  archived                              = var.archived
  extra_topics                          = var.extra_topics
  template                              = var.template
  admin_collaborators                   = var.admin_collaborators
  push_collaborators                    = var.push_collaborators
  pull_collaborators                    = var.pull_collaborators
  triage_collaborators                  = var.triage_collaborators
  maintain_collaborators                = var.maintain_collaborators
  admin_team_ids                        = var.admin_team_ids
  push_team_ids                         = var.push_team_ids
  pull_team_ids                         = var.pull_team_ids
  triage_team_ids                       = var.triage_team_ids
  maintain_team_ids                     = var.maintain_team_ids
  admin_teams                           = var.admin_teams
  push_teams                            = var.push_teams
  pull_teams                            = var.pull_teams
  triage_teams                          = var.triage_teams
  maintain_teams                        = var.maintain_teams
  branch_protections_v3                 = var.branch_protections_v3
  issue_labels_merge_with_github_labels = var.issue_labels_merge_with_github_labels
  issue_labels_create                   = var.issue_labels_create
  issue_labels                          = var.issue_labels
  deploy_keys                           = var.deploy_keys
  deploy_keys_computed                  = var.deploy_keys_computed
  projects                              = var.projects
  webhooks                              = var.webhooks
  plaintext_secrets                     = var.plaintext_secrets
  encrypted_secrets                     = var.encrypted_secrets
  autolink_references                   = var.autolink_references
  archive_on_destroy                    = var.archive_on_destroy
  module_depends_on                     = var.module_depends_on
}
